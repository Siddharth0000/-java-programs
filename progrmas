import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		System.out.println("enter youe age:");
		int age=sc.nextInt();
		if(age>18 && age<30)
		{
		    System.out.println("young adult");
		}
		else if (age>30)
		{
		    System.out.print("Adult ");
		}
		else {
		    System.out.print("child");
		}
		
		
	}
}
